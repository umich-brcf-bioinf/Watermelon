#%Module 1.0
#
#  cmake  module for use with 'environment-modules' package:
#

module-whatis "Loads RNASeq pipeline dependencies"

set	version		0.1
set	app		rnaseq

conflict $app

module load perl/5.22.2
module unload python
module load python/2.7.9
module load fastqc/0.11.3
module load bowtie2/2.2.1
module load boost/1.57.0 tophat/2.0.13
module load cufflinks/2.2.1
module load cutadapt/1.8.1
module load R/3.1.2
module load picard/1.77
module load samtools/0.1.19
